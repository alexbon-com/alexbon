{
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://www.alexbon.com/",
      "permanent": true
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/ua",
      "destination": "/ua.html"
    }
  ]
}